@article{crane_plant_1982,
	title = {Plant utilization at {Spoonbill}, an {Early} {Caddo} site in {Northeast} {Texas}},
	volume = {7},
	url = {https://www.jstor.org/stable/20707882},
	abstract = {Macrobotanical remains recovered by flotation from pit features indicate that the Early Caddo population at Spoonbill relied heavily upon the gathering of wild plant foods. Nut crops (particularly hickory nuts) were important wild plant foods. Also important were the small seeds of pioneer annuals, including Chenopodium (goosefoot), Polygonum (knotweed), and Iva (marsh elder); and fruits such as grapes, plums, persimmons, and passionflower provided minor supplements to the diet. Maize was grown by the Spoonbill people, but only a small quantity of maize remains were found. No other domesticated plant remains were recovered. It is proposed that domesticates constituted a larger portion of the diet during the Late Caddo period.},
	language = {English},

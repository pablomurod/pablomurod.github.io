@article{devall_biological_1992,
	title = {The biological flora of coastal dunes and wetlands. 2. {Ipomoea} pes-caprae ({L}.) {Roth}},
	volume = {8},
	copyright = {Copyright © 1992 Coastal Education \& Research Foundation, Inc.},
	issn = {0749-0208},
	url = {http://www.jstor.org/stable/4297988},
	abstract = {Ipomoea pes-caprae (L.) Roth (railroad vine) is a pantropical, perennial, trailing vine (Convolvulaceae with showy pink flowers that grows on coastal beaches and dunes throughout the tropical and subtropica areas of the world. Here I present a summary of ecological, physiological and geomorphological aspect of the biology of I. pes-caprae. Its success as a colonizer of tropical shorelines may be attributed to thi production of large numbers of fruits, long life cycle, high potential for water dispersal, resistance to sal spray, rapid growth, large neighborhood size, and vegetative reproduction. The species is important as j sand colonizer and plays a significant role in geomorphology. The plants often cover large areas and forn a rather continuous mat that helps to keep the sand from moving and holds the substrate together. The flowers are visited by bees and other insects; Xylocopa species (Hymenoptera) are important pollinators Ants visit extrafloral nectaries and help the plant increase reproductive success in some areas. The leave and seeds contain the indole alkaloid ergotamine, which protects them from many herbivores. Flower are sometimes eaten by herbivores, and a beetle (Megacerus leucospilus) parasitizes the seeds. /// Ipomoea pes-caprae (L.) Roth ist eine pantropische, perennierende Windepflanze (Convolvulaceae) mit auffälligen rosafarbenen Blüten, die weltweit auf allen tropischen und subtropischen Meeresstränden und Dünen wächst. In diesem Aufsatz wird eine Zusammenfassung ökologischer, physiologischer und geomorphologischer Aspekte im Leben von I. pes-caprae gegeben. Der Erfolg als Kolonisator von tropischen Stränden mag in der Tatsache begründet sein, daß eine große Anzahl von Früchten produziert wird, ein langer Lebenszyklus, ein hohes Potential zur Wasserverteilung und eine Resistenz gegen Salzaufwehung existiert, weiterhim ein schnelles Wachstum stattfindet, der eingenommene Siedlungsraum der Pflanzengesellschaft groß ist und eine vegatative Vermehrung möglich ist. Diese Pflanzenart ist ein wichtiger Kolonisator auf sandigem Untergrund und spielt aus deshalb auch eine bedeutsame Rolle bei geomorphologischen Prozessen im Strand- und Dünenbereich. Die Pflanzen bedecken oft ausgedehnte Gebiete und bilden eine weitestgehend zusammenhängende Matte, die deutlich zur Verminderung der Sandbewegung bzw. Fixierung des Seidmentes beiträgt. Die Blüten werden von Bienen und anderen Insekten besucht. Eine wichtige Funktion haben hier Xylocopa-Arten, die zu den Hymenóptera (Hautflügler) gehören. Ameisen suchen extraflorale Nektarien (Honigdrüsen außerhalb der Blüten) auf und helfen somit die Vermehrungsrate in manchen Gebieten zu erhöhen. Die Blätter und Samen enthalten das Indolalkaloid Ergotamin, welches sie vor vielen Pflanzenfressern schützt. Die Blüten werden manchmal von herbivoren Tieren gefressen und der Käfer (Megacerus leucospilus) ist ein Samenparasit bei I. pes-caprae. /// Ipomoea pes-caprae (L) Roth est une vigne pantropicale, perenne et grimpante (Convolvulacées). Elle a des fleurs d'un rose éclatant et pousse sur les plages et les dunes littorales des zones tropicales et subtropicales. On présente un résumé des facteurs écologiques, physiologiques et géomorphologiques de la biologie de Ipomoea pes-caprae. Son succès à coloniser les littoraux tropicaux peut être attribué à ce qu'elle produit un grand nombre de fruits, qu'elle a un cycle de vie long, un haut potentiel de dispersion de l'eau, qu'elle résiste aux embruns, croît rapidement, est fortement colonisatrice et a une reproduction végétative. Cette espèce, colonisatrice des sables, joue un rôle essentiel pour la géomorphologie. Elle couvre de vastes zones et forme un tapis assez continu qui fixe les sables et maintient le subtrat. Les fleurs, visitées par les abeilles et autres insectes. Xylocopa (Hyménoptères) sont très pollinifères. Les fourmis visitent les nectars extrafloraux et contribuent au succès de la croissance de la reproduction dans certaines zones. Les feuilles et les graines contiennent de l'ergotamine, alkaloïde indol qui les protège contre de nombreux herbivores. Les fleurs sont parfois mangées par les herbivores et un coléoptère (Megacerus leucospilus) parasite les graines.},
	language = {English},
	number = {2},
	urldate = {2015-01-20},
	journal = {Journal of Coastal Research},
	author = {Devall, Margaret S.},
	month = apr,
	year = {1992},
	keywords = {already processed - pending to add records to database, Ipomoea pes-caprae (L.) R.Br., Ipomoea imperati (Vahl) Griseb.},
	pages = {442--456},
}

@article{castresana_selection_2000,
	title = {Selection of conserved blocks from multiple alignments for their use in phylogenetic analysis},
	volume = {17},
	url = {http://molevol.cmima.csic.es/castresana/pdf/Castresana_2000.pdf},
	abstract = {The use of some multiple-sequence alignments in phylogenetic analysis, particularly those that are not very well conserved, requires the elimination of poorly aligned positions and divergent regions, since they may not be homologous or may have been saturated by multiple substitutions. A computerized method that eliminates such positions and at the same time tries to minimize the loss of informative sites is presented here. The method is based on the selection of blocks of positions that fulfill a simple set of requirements with respect to the number of contiguous conserved positions, lack of gaps, and high conservation of flanking positions, making the final alignment more suitable for phylogenetic analysis. To illustrate the efficiency of this method, alignments of 10 mitochondrial proteins from several completely sequenced mitochondrial genomes belonging to diverse eukaryotes were used as examples. The percentages of removed positions were higher in the most divergent alignments. After removing divergent segments, the amino acid composition of the different sequences was more uniform, and pairwise distances became much smaller. Phylogenetic trees show that topologies can be different after removing conserved blocks, particularly when there are several poorly resolved nodes. Strong support was found for the grouping of animals and fungi but not for the position of more basal eukaryotes. The use of a computerized method such as the one presented here reduces to a certain extent the necessity of manually editing multiple alignments, makes the automation of phylogenetic analysis of large data sets feasible, and facilitates the reproduction of the final alignment by other researchers.},
	language = {English},
	number = {4},
	journal = {Molecular Biology and Evolution},
	author = {Castresana, Jose},
	year = {2000},
	keywords = {already processed - no specimen citations, Gblocks},
	pages = {540--552},
}


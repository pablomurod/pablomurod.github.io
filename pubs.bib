@article{elmore_mode_1986-1,
	title = {Mode of reproduction and inheritance of leaf shape in \textit{{Ipomoea} hederacea}},
	volume = {34},
	issn = {0043-1745, 1550-2759},
	url = {https://www.cambridge.org/core/product/identifier/S0043174500067047/type/journal_article},
	doi = {10.1017/S0043174500067047},
	abstract = {Two distinctive leaf forms have been described in the morningglory species Ipomoea hederacea (L.) Jacq. These are identified as ivyleaf morningglory [I. hederacea (L.) Jacq.] and entireleaf morningglory [I. hederacea (L.) Jacq. var. integriuscula Gray]. Controlled matings were made between pure breeding forms of plants with these two leaf types. The first generation progeny (F1) were all ivyleaf and the second generation (F2) was segregated in a 3:1 ratio (ivyleaf to entireleaf). The data fit a single gene model for determination of leaf shape, with ivyleaf being the dominant allele. Despite the dominance of the ivyleaf trait, the abundance of the entireleaf phenotype in the Mississippi Delta was higher than that of ivyleaf. Ipomoea hederacea is a facultative self-pollinator and other annual morningglory species found in the same area are also self-pollinated.},

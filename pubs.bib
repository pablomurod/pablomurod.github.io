@article{pearse_native_2014,
	title = {Native plant diversity increases herbivory to non-natives},
	volume = {281},
	issn = {0962-8452, 1471-2954},
	url = {http://rspb.royalsocietypublishing.org/content/281/1794/20141841},
	doi = {10.1098/rspb.2014.1841},
	abstract = {There is often an inverse relationship between the diversity of a plant community and the invasibility of that community by non-native plants. Native herbivores that colonize novel plants may contribute to diversity–invasibility relationships by limiting the relative success of non-native plants. Here, we show that, in large collections of non-native oak trees at sites across the USA, non-native oaks introduced to regions with greater oak species richness accumulated greater leaf damage than in regions with low oak richness. Underlying this trend was the ability of herbivores to exploit non-native plants that were close relatives to their native host. In diverse oak communities, non-native trees were on average more closely related to native trees and received greater leaf damage than those in depauperate oak communities. Because insect herbivores colonize non-native plants that are similar to their native hosts, in communities with greater native plant diversity, non-natives experience greater herbivory.},
	language = {en},
	number = {1794},
	urldate = {2014-09-22},
	journal = {Proceedings of the Royal Society B: Biological Sciences},
	author = {Pearse, Ian S. and Hipp, Andrew L.},
	month = jul,
	year = {2014},
	pmid = {25232143},
	keywords = {biotic resistance hypothesis, enemy release hypothesis, host breadth, invasions, phylogenetic relatedness},
	pages = {20141841},
}


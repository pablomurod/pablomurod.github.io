@article{nagy_evodevo_2014,
	title = {Evodevo and the promise of understanding morphological transitions in evolution},
	volume = {99},
	issn = {0026-6493, 2162-4372},
	url = {http://www.bioone.org/action/showMultipleAbstracts?doi=10.3417%2F2011036&href=%2Ftoc%2Fmobt%2F99%2F3&mailPageTitle=Table+of+Contents+for+Annals+of+the+Missouri+Botanical+Garden%3A+Volume+99%2C+Issue+3+%28May+2014%29},
	doi = {10.3417/2011036},
	abstract = {The field of Evolutionary Developmental biology arose with the promise of new approaches to answering longstanding questions of comparative biology. Here we review the fruits of that promise some decades later. We chose three areas of arthropod EvoDevo—evolution of body plans, segment number, and appendage morphology—to provide an overview for the nonspecialist of how these issues have been clarified by the comparative analysis of regulatory gene networks. In all cases, we identify substantial progress and novel insights provided by the tools and perspective of EvoDevo. We also recognize that some core questions remain unanswered, and we reflect on how discoveries in EvoDevo fit in the landscape of other progress in phylogenetics, population biology, and genomics, facilitated by a new and ever-expanding set of molecular tools for comparative studies in evolution.},
	language = {en},
	number = {3},
	urldate = {2014-07-08},
	journal = {Annals of the Missouri Botanical Garden},
	author = {Nagy, Lisa M. and Williams, Terri A.},
	month = may,
	year = {2014},
	keywords = {Appendage development, arthropods, development, evolution, segmentation},
	pages = {289--300},
}


@phdthesis{velasquez_franco_13000_2015,
	address = {Bogotá, Colombia},
	type = {Masters' thesis},
	title = {13000 años de historia de vegetación, ocupación humana y volcanes - análisis paleoecológico del sitio arqueológico {Cuba}, {Valle} {Medio} del {Río} {Cauca}, {Colombia}},
	url = {http://biblioteca.uniandes.edu.co/acepto201699.php?id=5766.pdf},
	abstract = {En este estudio se presenta el primer análisis multiproxy de una sección de sedimentos (175 cm) proveniente del sitio arqueológico Cuba (66PER001), que permite revisar la idea de estabilidad climática y ecológica para los bosques premontanos de la Cordillera Central de Colombia durante el Holoceno, y de esta manera enmarcar los procesos de poblamiento temprano y domesticación de plantas en el noroccidente de Suramérica. Las líneas de evidencia independientes que se analizaron para la sección Cuba, muestran cambios en las condiciones ambientales desde finales del Pleistoceno (ca. 13,3 kaAP.) al presente. En general, las concentraciones de polen y esporas en la sección Cuba muestran un efecto de degradación del componente orgánico del suelo a lo largo de la columna estratigráfica. Las tefras presentan concentraciones relativamente altas en la base de la sección, con dos aumentos de concentración importantes en 164 cm y en 95 cm respectivamente. La actividad de fuegos (carbones sedimentarios {\textless}100 y {\textgreater}100μm), presentan valores altos en la base hasta los 12 cm, una marcada disminución alcanzando los valores mínimosentre 120 cm y 65 cm y un nuevo aumento en el tope entre 65 cm y 0 cm.Los valores de isótopos δ13C cambian desde valores muy negativos (-26,4) en la base de la sección (174 cm), hasta valores menos negativos (-22,48) en los 20 cm superiores de la sección. Los datos de la sección Cuba sugieren que la vegetación parece estar respondiendo a los cambios climáticos regionales, particularmente a los cambios de precipitación asociados con la migración de la zona de convergencia Intertropical (máximo térmico Holocénico) y el aumento de la variabilidad en fenómenos del Pacífico Oriental (ENOS).},
	language = {Español},
	school = {Universidad de Los Andes},
	author = {Velásquez Franco, Susana},
	year = {2015},
}


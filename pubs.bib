@phdthesis{aceituno_ocupaciones_2002,
	address = {Madrid},
	type = {Doctoral {Thesis}},
	title = {Ocupaciones tempranas del bosque tropical subandino en la {Cordillera} {Centro}-{Occidental} de {Colombia}},
	abstract = {La tesis aborda el estudio de cazadores-recolectores que ocuparon bosques tropicales de montaña en el Porce medio y en el Cauca medio, ambas regiones localizadas en la Cordillera centro-occidental de Colombia entre aproximadamente el 9500 hasta el 4000 B.P. El objetivo principal del trabajo es reconstruir las estrategias de explotación de este tipo de biomas considerados marginales para la ocupación humana. De este modo se pudo demostrar que los cazadores-recolectores perturbaron el bosque desde su ocupación con el fin de incrementar la capacidad de carga del medio, lo que con el tiempo desemboca en la producción artificial de alimentos a través de la dispersión intencional de taxas vegetales autóctonas.

No obstante, este cambio no hay que verlo como una ruptura en las formas de vida, sino más bien como una estrategia que forma parte de un continuum con la recolección y con la caza; es decir, aquellos cazadores-recolectores continuaron con su modelo de ocupación del territorio basado principalmente en mantener la movilidad como una estrategia eficaz de acceso a los recursos, así como a la interacción con otras bandas de territorios compartidos.

Tales inferencias fueron posibles a través de la aplicación de varias metodologías de análisis de registro arqueológico tales como la paleobotánica, la arqueozoología, el estudio de la industria lítica, procesos de formación de sitio, dataciones radiométricas, análisis espacial y algoritmos estadísticos.},
	language = {Español},
	school = {Universidad Complutense de Madrid},
	author = {Aceituno, Francisco J.},
	year = {2002},
	keywords = {PENDING TO READ},
}

@article{harris_divergent_2022,
	title = {Divergent evolutionary trajectories of bryophytes and tracheophytes from a complex common ancestor of land plants},
	issn = {2397-334X},
	url = {https://www.nature.com/articles/s41559-022-01885-x},
	doi = {10.1038/s41559-022-01885-x},
	abstract = {Abstract
            The origin of plants and their colonization of land fundamentally transformed the terrestrial environment. Here we elucidate the basis of this formative episode in Earth history through patterns of lineage, gene and genome evolution. We use new fossil calibrations, a relative clade age calibration (informed by horizontal gene transfer) and new phylogenomic methods for mapping gene family origins. Distinct rooting strategies resolve tracheophytes (vascular plants) and bryophytes (non-vascular plants) as monophyletic sister groups that diverged during the Cambrian, 515–494 million years ago. The embryophyte stem is characterized by a burst of gene innovation, while bryophytes subsequently experienced an equally dramatic episode of reductive genome evolution in which they lost genes associated with the elaboration of vasculature and the stomatal complex. Overall, our analyses reveal that extant tracheophytes and bryophytes are both highly derived from a more complex ancestral land plant. Understanding the origin of land plants requires tracing character evolution across a diversity of modern lineages.},
	language = {en},
	urldate = {2022-10-21},
	journal = {Nature Ecology \& Evolution},
	author = {Harris, Brogan J. and Clark, James W. and Schrempf, Dominik and Szöllősi, Gergely J. and Donoghue, Philip C. J. and Hetherington, Alistair M. and Williams, Tom A.},
	month = sep,

@phdthesis{canal_comparison_2020,
	address = {Ann Arbor, US},
	type = {Masters' thesis},
	title = {Comparison of {SNP} and {SSR} molecular marker systems in sweetpotato (\textit{{Ipomoea} batatas} {Lam}.) germplasm analysis and quantitative trait loci analysis of anthocyanin production in potato (\textit{{Solanum} tuberosum} subsp. \textit{tuberosum} {L}.) tubers},
	url = {https://search.proquest.com/docview/2405597806/abstract/1C1B3C03363E408BPQ},
	abstract = {The sweetpotato (Ipomoea batatas Lam.) and the potato (Solanum tuberosum subsp. tuberosum L.) have longstanding histories as staple crops, have undergone thousands of years of domestication and cultivation, provide high caloric yield per acre, are dense with essential nutrients, and they are both asexually propagated complex polyploids. Although they have many small differences, both crops serve similar purposes, and from a breeding perspective, similar breeding strategies are used for crop improvement. The studies of the genetic diversity of the sweetpotato breeding germplasm maintained at NCSU and the inheritance of the nutritional anthocyanin pigments produced in a biparental potato mapping population reported herein, were conducted to: 1) increase our understanding of genetic diversity present in the NC State sweetpotato breeding program so that the program can more efficiently harness the diversity to create new varieties; and 2) identify quantitative trait loci (QTL) for anthocyanin production in potato that can be used for marker-assisted breeding of purple-fleshed potatoes for the specialty potato market sector.

Maintaining and understanding germplasm diversity is crucial for breeding programs. Sweetpotato is a staple food source for global communities and its complex genetics pose steep challenges for elite cultivar development. We compared simple sequence repeat (SSR) and single nucleotide polymorphisms (SNP) to evaluate the advantages and disadvantages of each marker system for assessing genetic relationships amongst sweetpotato germplasm from different growing regions and breeding programs. Structure cluster analysis, comparison of genetic distance matrices and phylogenetic trees, and principal component analysis were used to assess the population structure predicted by the marker systems. We observed that the 1,312 SNP markers, developed in this study were denser and more evenly distributed throughout the genome, and they characterized the germplasm more effectively compared to the 100 polymorphic bands of the 11 SSR markers. The SSR markers established a population structure of 2 major clades divided into 7 clusters. The SNP markers established a population structure of 3 major clades divided into 8 clusters, with some clones showing genetic admixture.

Demand for anthocyanin-rich foods has increased over the past few decades following the increase in literature surrounding the possible health benefits of their antioxidant functionality. With potato being such an important staple crop, breeding for higher anthocyanin content could be a rapid way to include more nutrition in areas that have poor diets. Electrospray ionization-Time of flight/Mass spectrometry (ESI-TOF/MS) and high-performance liquid chromatography (HPLC) were utilized to characterize, identify, and quantify 19 constituent anthocyanin pigments and total monomeric anthocyanin (TMA) concentration in a biparental genetic mapping population in potato, segregating for anthocyanin accumulation. Linkage analysis of SNP markers derived from an Illumina Infinium 8303 Potato Single Nucleotide Polymorphism (SNP) Array was used to build a 1056 cM genetic map of the cross with 12 homologous linkage groups (LG). A total of 36 quantitative trait loci (QTL) were identified for 18 traits associated with anthocyanin and anthocyanidin production in potato. Two QTL were associated with transcript regions that contain key hydroxylase enzymes for the biosynthesis of flavonoids. A third of the putative transcript regions correlated with the QTL contain possible transcription factors. Other putative associated regions were related to stress response, noncoding regions, protein modification enzymes, and sucrose phosphorylation and dephosphorylation enzymes.},
	language = {English},
	school = {North Carolina State University},
	author = {Canal, Adam Malcolm},
	year = {2020},
}

@phdthesis{kinczyk_discovery_2020,
	address = {Ann Arbor, US},
	type = {Masters' thesis},
	title = {Discovery and analysis of {QTL} for important processing traits and resistance to \textit{{Fusarium} oxysporum,} f.sp. \textit{batatas} in the {NCDM04}-0001 × {Covington} sweetpotato mapping population},
	url = {https://search.proquest.com/docview/2405488352/abstract/488FF287BE3451BPQ},
	abstract = {Sweetpotato is an important staple food that feeds millions, but despite our understanding of its genetic architecture and the inheritance of important traits has lagged behind that of most other major crops. Recent advances in polyploid quantitative genetics and bioinformatics have facilitated entry into a new era where quantitative trait loci (QTL) and candidate genes for specific traits in polyploid crops such as sweetpotato are becoming more feasible. These new tools have become critical components for advancing cultivar development by helping us to understand the genetic factors underlying both simple and complex traits in sweetpotato. In this study, a biparental mapping population (DC) was created by crossing the light yellow-fleshed, high dry matter breeding line NCDM04-0001 (female) with the orange-fleshed, low dry matter cultivar Covington (male). Genotyping by sequencing (GBS), via a modified protocol called GBSpoly, was used to identify high-quality single nucleotide polymorphisms (SNPs) in the DC population from Illumina sequence data. The SNPs were analyzed utilizing novel genetic linkage mapping and QTL analysis algorithms (MAPpoly and QTLpoly respectively) for complex polyploids to develop a highdensity phased and integrated genetic linkage map of the DC population made up of 15 linkage groups that was phenotyped for important sweetpotato traits including the disease Fusarium oxysporum f.sp. batatas (Fob), and the storage root quality traits β-carotene, sucrose, glucose, and fructose.

Analyses of the mapping population revealed two significant QTL for Fob on linkage groups 10 and 3, responsible for {\textasciitilde}34\% and {\textasciitilde}10\% of total phenotypic variation within the progeny. Based on the combined molecular and phenotypic data it is hypothesized that resistance to Fob is a polygenic trait in sweetpotato, a finding common in plant disease resistance studies.

For β-carotene, sucrose, and fructose we identified three QTL each on linkage groups 2, 3, and 12: 1) On linkage groups 3, 4, and 12 for β-carotene; 2) On linkage groups 1 and 3 (two QTL) for sucrose; and 3) On linkage groups 1 and 3 for fructose. For glucose four QTL were identified on linkage groups 1, 3 (two QTL), and 12. In total, these linkage groups accounted for 82.1\% (β- carotene), 69.2\% (fructose), 71.8\% (sucrose), and 71.0\% (glucose) of total phenotypic variation. Simultaneously, we attempted to ascertain candidate genes responsible for these four traits utilizing recently developed reference genomes of sweetpotato relatives Ipomoea trifida and I. triloba. Using this information, we hope to develop methods for genomic-assisted breeding in the sweetpotato breeding program.},
	language = {English},
	school = {North Carolina State University},
	author = {Kinczyk, Jonathan Paul},
	year = {2020},
}


@article{iwanaga_use_1991,
	title = {Use of \textit{{Ipomoea} trifida} ({HBK}.) {G}. {Don} germ plasm for sweet potato improvement. 1. {Development} of synthetic hexaploids of \textit{{I}. trifida} by ploidy-level manipulations},

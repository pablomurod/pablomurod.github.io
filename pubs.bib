@phdthesis{dickau_resource_2005,
	address = {Philadelphia, Pennsylvania, US},
	type = {Doctoral {Thesis}},
	title = {Resource use, crop dispersals, and the transition to agricultrue in prehistoric {Panama}: evidence from starch grains and macroremains},
	url = {https://search.proquest.com/docview/305393070},
	abstract = {Increasing amounts of microbotanical and molecular evidence indicate that humans occupying lowland tropical regions played a central role in the origins of plant domestication and food production in the Americas.  Macroremain and starch grain analyses were carried out on previously excavated stone tools from 15 archaeological sites in Panama, to investigate wild resource use, patterns of crop dispersals throughout Panama and the Neotropics, and the transition to food production in western Pacific Panama (Gran Chiriquí), and central Pacific Panama (Gran Coclé). Results indicate that inhabitants of both regions were using a broad spectrum of local resources during the initial cultivation of exogamous crops, including several types of palm and tree fruits, wild yams (Dioscorea sp.), Calathea, and Zamia (in Gran Chiriquí).  Many of these resources continued to be used after the adoption of domesticates, contributing to the diet breadth of the prehistoric populations.   Domesticated species were adopted in both regions during preceramic periods.  In Gran Chiriquí, arrowroot (Maranta arundinacea) and maize (Zea mays) were available by 6000 BP, and manioc (Manihot esculenta) by 5000 BP.  Starch grain evidence of maize predates macroremains by several millennia, and alters previous suggestions of a late introduction of agriculture in the region.  In Gran Coclé, starch data confirm previous microbotanical evidence for the presence of maize ca. 7000 BP, and its subsequent widespread cultivation.  Manioc, arrowroot, and yams (Dioscorea trifida) were alsocultivated and processed.  Macrobotanical evidence shows that cacao (Theobroma cacao) was grown by 3300 BP in Gran Coclé. Research indicates that crop dispersed relatively rapidly throughout the Neotropics, and independently of one another.  This suggests that most dispersals occurred via diffusion rather than population migration.  When new crops arrived in Gran Chiriquí, the transition to food production appears to have been initially less intense than in Gran Coclé, possibly due to lower population density.  However, in both regions the transition from initial adoption of domesticates to reliance on agriculture and settled village life took several thousand years.  A complex interplay between wild resource availability, crop productivity, population density, and environmental factors directed the nature and pace of this transition.},
	language = {English},
	school = {Temple University},
	author = {Dickau, Ruth},
	year = {2005},
}


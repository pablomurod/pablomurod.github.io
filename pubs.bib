@article{lopez-curto_life_1990,
	title = {Life cycle and seed development of \textit{{Ipomoea}} × \textit{leucantha} ({Convolvulaceae}), a weed of rice},
	volume = {51},
	abstract = {In the laboratory, Ipomoea × leucantha had a life cycle of about 115 days. Anthesis occurred 70-90 days after germination; seeds were mature, dry and ready for dispersal about 25 d after anthesis. The ovary contained 4 basic anatropic ovules. The single tegument differentiated into the testa consisting at maturity of 4 coverings: the epidermis, and subepidermis which are single-layered, the sclerenchyma with 3 to 4 layers of cells and a fibrous covering consisting of collapsed parenchyma. The outer layer of the sclerenchyma has a double line light in colour. When mature the endosperm has only 2-3 layers of living cells (aleurone layer) and the remains of the cell walls. The first division of the zygote is transverse. The embryo with its cotyledons occupies the greater part of the mature seed and has many laticifers.},
	number = {1},
	journal = {Phyton (Buenos Aires)},
	author = {López-Curto, L. and Márquez-Guzmán, J. and Laguna-Hernández, G. and Ponce-Salazar, M.},
	year = {1990},

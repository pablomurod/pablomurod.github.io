@article{raghavan_genomic_2015,
	title = {Genomic evidence for the {Pleistocene} and recent population history of {Native} {Americans}},
	volume = {349},
	issn = {0036-8075, 1095-9203},
	url = {https://www.science.org/doi/10.1126/science.aab3884},
	doi = {10.1126/science.aab3884},
	abstract = {Several theories have been put forth as to the origin and timing of when Native American ancestors entered the Americas. To clarify this controversy, Raghavan et al. examined the genomic variation among ancient and modern individuals from Asia and the Americas. There is no evidence for multiple waves of entry or recurrent gene flow with Asians in northern populations. The earliest migrations occurred no earlier than 23,000 years ago from Siberian ancestors. Amerindians and Athabascans originated from a single population, splitting approximately 13,000 years ago.},

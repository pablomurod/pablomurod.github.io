@article{ozias-akins_nuclear_1994,
	title = {Nuclear {DNA} content and ploidy levels in the genus \textit{{Ipomoea}}},
	volume = {119},
	url = {https://www.google.com/url?sa=t&rct=j&q=&esrc=s&source=web&cd=&ved=2ahUKEwiqjIzpr971AhWznVwKHVgqCZsQFnoECAkQAQ&url=https%3A%2F%2Fjournals.ashs.org%2Fdownloadpdf%2Fjournals%2Fjashs%2F119%2F1%2Farticle-p110.pdf&usg=AOvVaw2Di16TFacZYN5APymlwNOA},
	abstract = {The nuclear DNA content of 53 accessions from 24 Ipomoea (Convolvulaceae) species, including four sweetpotato cultivars, was determined by flow cytometry of DAPI-stained nuclei. Ploidy level and DNA content were significantly correlated within the genus, but more highly so within species that contained multiple cytotypes. DNA content of cultivated Z. batatas (L.) Lam. (4.8 to 5.3 pg/2C nucleus) and feral tetraploid I. batatas (3.0 to 3.5 pg/2C nucleus) was estimated from the known DNA content of chicken erythrocytes (2.33 pg), which were used as an internal standard. Tetraploid forms of Z. cordato-triloba Dennstedt also were identified. Ploidy analysis using flow cytometry is rapid and suitable for large-scale experiments such as studying the genetic structure of populations of Z. batatas and related species. Chemical name used: 4´,6-diamidino-2-phenylindole (DAPI).},
	language = {English},
	number = {1},
	journal = {Journal of the American Society for Horticultural Science},
	author = {Ozias-Akins, Peggy and Jarret, Robert L.},
	year = {1994},
	pages = {110--115},
}

